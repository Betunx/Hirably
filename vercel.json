{
  "version": 2,
  "buildCommand": "ng build --configuration production --base-href /",
  "outputDirectory": "dist/hirably/browser",
  "framework": null,
  "routes": [
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|svg|woff|woff2|ttf|eot))",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}